pipeline{ 
    agent any
        tools{
            maven 'maven'
        }

    stages{ 
           stage('git-checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/Swathi971/trail.git'
            }
           } 

           stage('compile'){
            steps{
                sh 'mvn compile'
            }
           }

            stage('build'){
                steps{
                    sh 'mvn clean package'
                }
            }
           }
        } 
    