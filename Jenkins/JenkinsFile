pipeline { 
    agent any
    tools {
        maven 'maven'
        jdk 'java-11'
    }

    stages { 
        stage('Git Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Swathi971/trail.git'
            }
        } 

        stage('Compile') {
            steps {
                dir('Jenkins') {  // Navigate to the "Jenkins" folder
                    sh 'mvn compile'
                }
            }
        }

        stage('Build') {
            steps {
                dir('Jenkins') {  
                    sh 'mvn clean package'
                }
            }
        }
    }
}

    